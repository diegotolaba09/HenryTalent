{
  "name": "talent-api",
  "version": "1.0.0",
  "description": "Henry Talent API",
  "private": true,
  "license": "UNLICENSED",
  "main": "server/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "prettier": "prettier -c -w \"{server,test}/**/*.{js,ts,tsx}\"",
    "test": "mocha -r ts-node/register -w test/**/*.ts --delay",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit",
    "ci": "mocha -r ts-node/register test/**/*.ts --delay --exit",
    "seed": "sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@types/node": "^14.14.7",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.2.0",
    "faker": "^5.1.0",
    "fast-csv": "^4.3.5",
    "mailgun-js": "^0.22.0",
    "multer": "^1.4.2",
    "pg": "^8.4.2",
    "pg-hstore": "^2.3.3",
    "pino": "^6.7.0",
    "sequelize": "^6.3.5",
    "tmp": "^0.2.1",
    "uuidv4": "^6.2.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/mailgun-js": "^0.22.11",
    "@types/mocha": "^8.0.3",
    "@types/pino": "^6.3.2",
    "@types/sequelize": "^4.28.9",
    "@types/shelljs": "^0.8.8",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "chai": "^4.2.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.1.2",
    "sequelize-cli": "^6.2.0",
    "shelljs": "^0.8.4",
    "supertest": "^5.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "author": "Henry <devs@soyhenry.com> (https://soyhenry.com)"
}
